class Person{
    constructor(name,age){
        this.name=name;
        this.age=age;
    }
}
class President extends Person{
    constructor(name,age,country){
        super(name,age);
        this.country=country;
    }
}
class Footballer extends Person{
    constructor(name,age,goals){
        super(name,age);
        this.goals=goals
    }
}
